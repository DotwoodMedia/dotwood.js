{
  "name": "dotwood.js",
  "version": "3.0.0-dev.0",
  "description": "This framework makes creating a bot much easier! With this module you can create a good basis for bot",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DotwoodMedia/dotwood.js.git"
  },
  "keywords": [
    "discord.js",
    "framework",
    "typescript"
  ],
  "author": "Dotwood Media",
  "license": "Apache",
  "bugs": {
    "url": "https://github.com/DotwoodMedia/dotwood.js/issues"
  },
  "homepage": "https://github.com/DotwoodMedia/dotwood.js#readme",
  "dependencies": {
    "discord.js": "^14.7.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "prettier": "^2.8.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.4"
  }
}
