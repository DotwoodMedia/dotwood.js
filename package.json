{
  "name": "dotwood.js",
  "version": "2.1.3",
  "description": "This framework makes creating a bot much easier! With this module you can create a good basis for bot",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DotwoodMedia/dotwood.js.git"
  },
  "keywords": [
    "discord.js",
    "framework",
    "dbot"
  ],
  "author": "Dotwood Media",
  "license": "Apache",
  "bugs": {
    "url": "https://github.com/DotwoodMedia/dotwood.js/issues"
  },
  "homepage": "https://github.com/DotwoodMedia/dotwood.js#readme",
  "dependencies": {
    "@discordjs/rest": "^0.1.0-canary.0",
    "@discordjs/voice": "^0.6.0",
    "chalk": "^4.1.1",
    "discord-api-types": "^0.22.0",
    "discord.js": "^13.2.0",
    "fs": "0.0.1-security",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "prettier": "^2.8.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.4"
  }
}
